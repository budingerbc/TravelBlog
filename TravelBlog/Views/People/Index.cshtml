@model IEnumerable<TravelBlog.Models.People>

@{
    ViewData["Title"] = "People";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>People</h1>

<table class="table">
	<tr>
		<th>@Html.DisplayNameFor(model => model.Name)</th>
		<th>@Html.DisplayNameFor(model => model.Experiences.Title)</th>
	</tr>

	@foreach(var person in Model)
	{
        <tr>
		    <td>@Html.DisplayFor(model => person.Name)</td>
		    <td>@Html.DisplayFor(model => person.Experiences.Title)</td>
	    </tr>
	}
</table>

<button>@Html.ActionLink("Add new person", "Create")</button>